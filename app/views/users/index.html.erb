<% flash.each do |key, value| %>
    <%if key == "error"%>
        <div class="alert alert-danger"><%= value %></div>
    <%else%>
        <div class="alert alert-success"><%= value %></div>
    <%end%>
<% end %>

  <div class="row">
    <div class="col-md-12 bgimage">
      <div class="bgimage-inside"></div>
    </div>
  </div>
</div>